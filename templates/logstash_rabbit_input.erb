  rabbitmq {
    auto_delete => false
    durable => true
    exchange => "logstash-exchange"
    exclusive => false
    host => "<%= @ipaddress %>"
    user => 'logstash'
    password => '<%= @rabbit_logstash_password %>'
    queue => "logstash-queue"
    tags => ['rabbitMQ-<%= @fqdn %>']
    type => "logging"
  }
